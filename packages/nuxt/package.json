{
  "name": "@logtide/nuxt",
  "version": "0.5.6",
  "description": "LogTide SDK integration for Nuxt â€” auto error capture, request tracing via Nitro hooks",
  "type": "module",
  "main": "./dist/module.cjs",
  "module": "./dist/module.js",
  "types": "./dist/module.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/module.d.ts",
        "default": "./dist/module.js"
      },
      "require": {
        "types": "./dist/module.d.cts",
        "default": "./dist/module.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/logtide-dev/logtide-javascript.git",
    "directory": "packages/nuxt"
  },
  "homepage": "https://github.com/logtide-dev/logtide-javascript#readme",
  "bugs": {
    "url": "https://github.com/logtide-dev/logtide-javascript/issues"
  },
  "author": "Polliog <giuseppe@solture.it>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@logtide/core": "workspace:*",
    "@logtide/types": "workspace:*"
  },
  "peerDependencies": {
    "nuxt": ">=3.0.0",
    "@nuxt/kit": ">=3.0.0"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.14.0",
    "@nuxt/schema": "^3.14.0",
    "nuxt": "^3.14.0",
    "tsup": "^8.5.1",
    "typescript": "^5.5.4"
  }
}
